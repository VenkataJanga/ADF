					"ls_sql_query_blob_connectionString": {
						"value": "[parameters('ls_sql_query_blob_connectionString')]"
					},
					"ls_sql_to_blob_connectionString": {
						"value": "[parameters('ls_sql_to_blob_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dtagdatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dtagdatafactory_ArmTemplate_0')]"
			]
		}
	]
}